{
  "psi-header.variables": [["projectCreationYear", "2020"]],
  "psi-header.config": {
    "forceToTop": true,
    "blankLinesAfter": 1,
    "company": " Royal-Kingdom-Vassals",
    "copyrightHolder": "Royal Kingdom Vassals LTD"
  },
  "psi-header.changes-tracking": {
    "autoHeader": "manualSave"
  },
  "psi-header.templates": [
    {
      "language": "*",
      "template": [
        "File: <<filename>>",
        "Project: <<projectname>>",
        "File Created: <<filecreated('dddd, Do MMMM YYYY h:mm:ss a')>>",
        "Author: <<author>> (<<authoremail>>)",
        "-----",
        "Last Modified: <<dateformat('dddd, Do MMMM YYYY h:mm:ss a')>>",
        "Modified By: <<author>> (<<authoremail>>>)",
        "-----",
        "Copyright <<projectCreationYear>> - <<year>> <<copyrightholder>>, <<company>>"
      ]
    }
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "editor.minimap.maxColumn": 80,
  "workbench.iconTheme": "vscode-icons",
  "peacock.color": "#a06004",
  "peacock.affectStatusBar": false,
  "peacock.affectSashHover": false,
  "peacock.affectActivityBar": false,
  "workbench.colorCustomizations": {
    "titleBar.activeBackground": "#a06004",
    "titleBar.inactiveBackground": "#a0600499",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.darkenLightenPercentage": 1
}
